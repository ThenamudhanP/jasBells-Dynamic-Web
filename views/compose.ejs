<%- include("Partials/header") -%>
<h1>COMPOSE</h1>
<p><%=composecontents %></p>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Name</label>
    <input type="text" class="form-control" name="posttitle" placeholder="Enter Your Name" />
    <label>Post</label>
    <textarea class="form-control" name="posttext" cols="30" rows="5"></textarea>
    <label>Password</label>
    <input type="password" class="form-control" name="postpassword" placeholder="Enter password" />
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>

</form>

<%- include("Partials/footer") -%>
